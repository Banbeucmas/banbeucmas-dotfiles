#!/bin/sh

initializeANSI(){
  esc=""
  reset="${esc}[0m"

  border="${reset}${esc}[38;5;232m"

  greyd="${reset}${esc}[38;5;235m"; grey="${reset}${esc}[38;5;240m"
  whited="${reset}${esc}[38;5;251m"; white="${reset}${esc}[38;5;255m"
  yellowd="${reset}${esc}[38;5;172m"; yellow="${reset}${esc}[38;5;214m"; yellowb="${reset}${esc}[38;5;226m"
  humand="${esc}[38;5;216m"; human="${esc}[38;5;223m"; humanb="${esc}[38;5;229m"
  brown="${esc}[38;5;130m"; brownb="${esc}[38;5;214m";
  eyed="${reset}${esc}[38;5;251m"; eye="${reset}${esc}[38;5;255m"

  boldon="${esc}[1m";    boldoff="${esc}[22m"
  italicson="${esc}[3m"; italicsoff="${esc}[23m"
  ulon="${esc}[4m";      uloff="${esc}[24m"
  invon="${esc}[7m";     invoff="${esc}[27m"
 
}

initializeANSI

